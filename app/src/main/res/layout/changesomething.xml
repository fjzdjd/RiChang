<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ddw.com.richang.Activity.myAccount.ChangeSomething">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
        <RelativeLayout
            android:background="#ffffff"
            android:layout_width="fill_parent"
            android:layout_height="56dp">
            <TextView
                android:layout_centerInParent="true"
                android:textSize="20sp"
                android:text="个人资料"
                android:textColor="@color/rorange"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <ImageView
                android:id="@+id/cancelchangesomething"
                android:layout_marginLeft="12dp"
                android:layout_centerVertical="true"
                android:src="@drawable/leftgo"
                android:paddingTop="15sp"
                android:paddingBottom="15sp"
                android:paddingRight="20dp"
                android:layout_width="wrap_content"
                android:layout_height="46sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/changeprofile"
            android:layout_marginTop="7dp"
            android:background="#ffffff"
            android:padding="16dp"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_centerVertical="true"
                android:text="头像"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_marginRight="3dp"
                    android:id="@+id/changeimgprofile"
                    android:layout_width="40dp"
                    android:layout_height="40dp" />
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/goright"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_height="12dp" />
            </LinearLayout>
        </RelativeLayout>
        <ImageView
            android:background="#8c8c8c"
            android:layout_width="fill_parent"
            android:layout_marginLeft="16dp"
            android:layout_height="1px" />

        <RelativeLayout
            android:background="#ffffff"
            android:padding="16dp"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="手机"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#8c8c8c"
                    android:layout_marginRight="3dp"
                    android:id="@+id/changetvphone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:visibility="invisible"
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/goright"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_height="12dp" />
            </LinearLayout>
        </RelativeLayout>
        <ImageView
            android:background="#8c8c8c"
            android:layout_width="fill_parent"
            android:layout_marginLeft="16dp"
            android:layout_height="1px" />

        <RelativeLayout
            android:id="@+id/changename"
            android:background="#ffffff"
            android:padding="16dp"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="昵称"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#8c8c8c"
                    android:layout_marginRight="3dp"
                    android:id="@+id/changetvname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/goright"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_height="12dp" />
            </LinearLayout>
        </RelativeLayout>
        <ImageView
            android:background="#8c8c8c"
            android:layout_width="fill_parent"
            android:layout_marginLeft="16dp"
            android:layout_height="1px" />

        <RelativeLayout
            android:id="@+id/changegender"
            android:background="#ffffff"
            android:padding="16dp"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="性别"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#8c8c8c"
                    android:layout_marginRight="3dp"
                    android:id="@+id/changetvgender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/goright"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_height="12dp" />
            </LinearLayout>
        </RelativeLayout>
        <ImageView
            android:background="#8c8c8c"
            android:layout_width="fill_parent"
            android:layout_marginLeft="16dp"
            android:layout_height="1px" />

        <RelativeLayout
            android:id="@+id/changemail"
            android:background="#ffffff"
            android:padding="16dp"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="邮箱"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#8c8c8c"
                    android:layout_marginRight="3dp"
                    android:id="@+id/changetvemail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/goright"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_height="12dp" />
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/changesign"
            android:layout_marginTop="7dp"
            android:background="#ffffff"
            android:padding="16dp"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="签名"
                android:textColor="@color/black"
                android:textSize="16sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#8c8c8c"
                    android:layout_marginRight="3dp"
                    android:id="@+id/changetvsign"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:src="@drawable/goright"
                    android:layout_width="wrap_content"
                    android:layout_marginLeft="3dp"
                    android:layout_height="12dp" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>
